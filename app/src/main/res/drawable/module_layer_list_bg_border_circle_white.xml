<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<!-- 边 -->
<item>
    <shape android:shape="rectangle">
        <padding
            android:bottom="1dp"
            android:left="1dp"
            android:right="1dp"
            android:top="1dp" />
        <solid android:color="#10dcf1f6" />
        <corners android:radius="80dp" />
    </shape>
</item>
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <solid android:color="#30dcf1f6" />
            <corners android:radius="80dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <solid android:color="#50dcf1f6" />
            <corners android:radius="80dp" />
        </shape>
    </item>


<!-- 中心背景 -->
<item>
    <shape android:shape="rectangle"
        android:useLevel="false">
        <!-- 实心 -->
        <solid android:color="@color/white" />
        <corners android:radius="80dp" />

    </shape>
</item>




</layer-list>
